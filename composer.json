{
  "authors": [
    {
      "name": "Michel Chowanski",
      "email": "michel.chowanski@bestit-online.de"
    }
  ],
  "autoload": {
    "psr-4": {
      "BestIt\\ContentfulTranslationBundle\\": "src/ContentfulTranslationBundle"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\BestIt\\ContentfulTranslationBundle\\": "tests/ContentfulTranslationBundle"
    }
  },
  "description": "A Translation bundle for loading messages from contentful",
  "license": "MIT",
  "minimum-stability": "dev",
  "name": "bestit/contentful-translation-bundle",
  "prefer-stable": true,
  "require": {
    "php": "^7.0",
    "symfony/symfony": "^3.1",
    "contentful/contentful": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.5",
    "phing/phing": "^2.16",
    "squizlabs/php_codesniffer": "^2.8",
    "sebastian/phpcpd": "^3.0",
    "phpmd/phpmd": "^2.6",
    "phploc/phploc": "^3.0"
  },
  "type": "lib"
}
